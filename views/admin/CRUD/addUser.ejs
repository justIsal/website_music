<%- include('../../layout/header_admin')%>
    <div class="my-3 w-full bg-slate-100 box p-4 flex top-10 right-0">
        <div class="m-auto w-full">
            <form action="/createUser" class="px-4 w-full" method="post">
                <p>username</p>
                <input type="text" class="text-black w-full h-10 bg-slate-200 rounded-md pl-2" name="username">
                <p>email</p>
                <input type="email" class="text-black w-full h-10 bg-slate-200 rounded-md pl-2" name="email">
                <p>password</p>
                <input type="password" class="text-black w-full h-10 bg-slate-200 rounded-md pl-2" name="password">
                <input type="submit"
                    class="w-full h-10 text-white bg-blue-400 rounded-md pl-2 cursor-pointer mt-3 hover:bg-blue-500"
                    name="submit" value="Tambah">
            </form>
        </div>
    </div>
    <%- include('../../layout/footer_admin')%>